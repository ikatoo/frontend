  "scripts": {
    "lint": "eslint src --max-warnings=0",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "playwright": "npx playwright test",
    "playwright:ui": "npx playwright test --ui",
    "playwright:codegen": "npx playwright codegen",
    "docker:vite:up": "docker compose -f docker/vite/compose.yml up -d",
    "docker:vite:down": "docker compose -f docker/vite/compose.yml down",
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "typecheck:watch": "tsc --project tsconfig.json --noEmit --watch",
    "preview": "vite preview",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@styled-icons/boxicons-logos": "10.46.0",
