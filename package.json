{
  "name": "ikatoo-backend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": {
    "name": "Milton Carlos Katoo",
    "url": "https://www.ikatoo.com.br",
    "email": "mckatoo@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint src --max-warnings=0",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "playwright": "bash docker/playwright/test.sh",
    "playwright:ui": "bash docker/playwright/ui.sh",
    "playwright:codegen": "bash docker/playwright/codegen.sh",
    "docker:vite:up": "docker compose -f docker/vite/compose.yml up -d",
    "docker:vite:down": "docker compose -f docker/vite/compose.yml down",
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "preview": "vite preview",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@react-google-maps/api": "2.13.1",
    "@styled-icons/boxicons-logos": "10.46.0",
